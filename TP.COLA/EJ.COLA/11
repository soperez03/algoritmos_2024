'''c


def insert(personajes, new):
    antesYoda= ("Yoda", "Dagobah")
    new_cola=[]
    for personaje in personajes:
        new_cola.append(personaje)
        if personaje == antesYoda:
            new_cola.append(new)
    return new_cola

